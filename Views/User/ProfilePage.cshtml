<script>
    $('#ok1').hide();
    $('#ok2').hide();
    $('#ok3').hide();
    $('#ok4').hide();
    $('#ok5').hide();
    $('#ok7').hide();
    $('#ok8').hide();
    $('#ok9').hide();
    $('#ok10').hide();
    $('#ok11').hide();
    $('#ok12').hide();
    $('#ok13').hide();
    $('#ok14').hide();
    $('#ok15').hide();
    $('#ok16').hide();
    $('#ok17').hide();
    $('#ok18').hide();
    $('.okImg').hide();

    $('.path').click(function () {

        $('.okImg').show();
        $('.path').hide();
        
        $('#edit1').hide();
        $('#edit2').hide();
        $('#edit3').hide();
        $('#edit4').hide();
        $('#edit5').hide();
        $('#edit7').hide();

    });

    $('.okImg').click(function () {
        
        $('.okImg').hide();
        $('#edit1').show();
        $('#edit2').show();
        $('#edit3').show();
        $('#edit4').show();
        $('#edit5').show();
        $('.path').show();
        $('#edit7').show();
        
    });


    $('#edit1').click(function () {
        $('#ok1').show();
        $('#edit1').hide();
        $('#fname1').text(' ');
        $('#fname1').append('<input type="text" name="fname" id="fname" placeholder="Enter First Name" size="20" required />');

        $('#edit2').hide();
        $('#edit3').hide();
        $('#edit4').hide();
        $('#edit5').hide();
        $('#edit7').hide();

    });

    $('#ok1').click(function () {

        var fname = $('#fname').val();

        $.getJSON("UpdateProfile?f=" + fname + "&n=1", function (data) {
            if (data == true) {
                $('#result').text("First Name Edit successfully");
                $('#fname1').text(' ' + fname);
            }
            else {

                $('#result').text("Error: Please try Later");
            }

            $('#ok1').hide();
            $('#edit1').show();
            $('#edit2').show();
            $('#edit3').show();
            $('#edit4').show();
            $('#edit5').show();
            $('#edit7').show();
        });

    });

    $('#edit2').click(function () {
        $('#ok2').show();
        $('#edit2').hide();
        $('#lname1').text(' ');
        $('#lname1').append('<input type="text" name="lname" id="lname" placeholder="Enter Last Name" size="20" required />');

        $('#edit1').hide();
        $('#edit3').hide();
        $('#edit4').hide();
        $('#edit5').hide();
        $('#edit7').hide();

    });

    $('#ok2').click(function () {

        var lname1 = $('#lname').val();

        $.getJSON("UpdateProfile?f=" + lname1 + "&n=2", function (data) {
            if (data == true) {
                $('#result').text("Last Name Edit successfully");
                $('#lname1').text(' ' + lname1);
            }
            else {

                $('#result').text("Error: Please try Later");
            }

            $('#ok2').hide();
            $('#edit1').show();
            $('#edit2').show();
            $('#edit3').show();
            $('#edit4').show();
            $('#edit5').show();
            $('#edit7').show();
        });

    });



    $('#edit3').click(function () {
        $('#ok3').show();
        $('#edit3').hide();
        $('#dob1').text(' ');
        $('#dob1').append('<input type="date" name="dob" id="dob" size="20" />');

        $('#edit2').hide();
        $('#edit1').hide();
        $('#edit4').hide();
        $('#edit5').hide();
        $('#edit7').hide();
    });

    $('#ok3').click(function () {

        var dob = $('#dob').val();

        $.getJSON("UpdateProfile?f=" + dob + "&n=3", function (data) {
            if (data == true) {
                $('#result').text("Date of Birth Edit successfully");
                $('#dob1').text(' ' + dob);
            }
            else {

                $('#result').text("Error: Please try Later");
            }

            $('#ok3').hide();
            $('#edit1').show();
            $('#edit2').show();
            $('#edit3').show();
            $('#edit4').show();
            $('#edit5').show();
            $('#edit7').show();
        });

    });


    $('#edit4').click(function () {
        $('#ok4').show();
        $('#edit4').hide();
        $('#gender1').text(' ');
        $('#gender1').append('<input type="radio" value="male" name="gender" checked /> Male  <br />' +
                                        '<input type="radio" value="female" name="gender" /> Female <br /><br />');

        $('#edit2').hide();
        $('#edit3').hide();
        $('#edit1').hide();
        $('#edit5').hide();
        $('#edit7').hide();
    });

    $('#ok4').click(function () {

        var gender = $("input[name=gender]:checked").val();

        $.getJSON("UpdateProfile?f=" + gender + "&n=4", function (data) {
            if (data == true) {
                $('#result').text("Gender Edit successfully");
                $('#gender1').text(' ' + gender);
            }
            else {

                $('#result').text("Error: Please try Later");
            }

            $('#ok4').hide();
            $('#edit1').show();
            $('#edit2').show();
            $('#edit3').show();
            $('#edit4').show();
            $('#edit5').show();
            $('#edit7').show();
        });

    });


    $('#edit5').click(function () {
        $('#ok5').show();
        $('#edit5').hide();
        $('#address1').text(' ');
        $('#address1').append('<input type="text" name="address" id="address" size="20" placeholder="Enter Address" />');

        $('#edit2').hide();
        $('#edit3').hide();
        $('#edit4').hide();
        $('#edit1').hide();
        $('#edit7').hide();
    });

    $('#ok5').click(function () {

        var address = $('#address').val();

        $.getJSON("UpdateProfile?f=" + address + "&n=5", function (data) {
            if (data == true) {
                $('#result').text("Address Edit successfully");
                $('#address1').text(' ' + address);
            }
            else {

                $('#result').text("Error: Please try Later");
            }

            $('#ok5').hide();
            $('#edit1').show();
            $('#edit2').show();
            $('#edit3').show();
            $('#edit4').show();
            $('#edit5').show();
            $('#edit7').show();
        });

    });


    $('#edit7').click(function () {
        $('#ok7').show();
        $('#edit7').hide();
        $('#phone1').text(' ');
        $('#phone1').append('<input type="text" name="phone" id="phone" size="20" placeholder="Enter Phone Number" onkeypress="return isNumberKey(event)" />');

        $('#edit2').hide();
        $('#edit3').hide();
        $('#edit4').hide();
        $('#edit5').hide();
        $('#edit1').hide();
    });

    $('#ok7').click(function () {

        var phone = $('#phone').val();

        $.getJSON("UpdateProfile?f=" + phone + "&n=7", function (data) {
            if (data == true) {
                $('#result').text("Phone Edit successfully");
                $('#phone1').text(' ' + phone);

            }
            else {

                $('#result').text("Error: Please try Later");
            }
            $('#ok7').hide();
