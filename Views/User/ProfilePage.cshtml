                                    <br />
                                    <div style="color:red" id="result2"></div>

                                    <div class="panel panel-info">
                                        <div class="panel-heading">
                                            <h3 class="panel-title">@Model.fname.ToString() @Model.lname.ToString()</h3>
                                        </div>
                                        <div class="panel-body">
                                            <div class="row">
                                                <form action="../User/ProfilePic" id="form1" method="post" enctype="multipart/form-data">
                                                    <div class="col-md-12 col-lg-12" align="center">
                                                        <img id="img1" alt="User Pic" src="@Model.path.ToString()" height="100" width="100" class="img-circle">
                                                        <input id="path2" name="path" data-toggle="tooltip" type="file" class="btn btn-sm btn-primary path" value="Edit">
                                                        <input type="hidden" id="hide1" name="hide1" />
                                                        <button name="okImg" id="okImg2" data-original-title="Broadcast Message" data-toggle="tooltip" type="submit" class="btn btn-sm btn-primary okImg">OK <i class="glyphicon glyphicon-ok"></i></button>
                                                    </div>
