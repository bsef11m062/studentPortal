                                                <div class="col-xs-10 col-sm-10 hidden-md hidden-lg">
                                                    <br>

                                                </div>
                                                <div class=" col-md-9 col-lg-12 ">
                                                    <table class="table table-user-information">
                                                        @{
                                                            <tbody>
                                                                <tr>
                                                                    <td>Username:</td>
                                                                    <td>@Model.username.ToString()</td>
                                                                </tr>
                                                                <tr>
                                                                    <td>Current Degree:</td>
                                                                    <td id="degree3">@ViewBag.x.degree.ToString()</td>
                                                                    <td>
                                                                        <button id="edit14" data-original-title="Broadcast Message" data-toggle="tooltip" type="button" class="btn btn-sm btn-primary">Edit  <i class="glyphicon glyphicon-edit"></i></button>
                                                                        <a id="ok14" data-original-title="Broadcast Message" data-toggle="tooltip" href="#" type="button" class="btn btn-sm btn-primary">OK <i class="glyphicon glyphicon-ok"></i></a>
                                                                    </td>

                                                                </tr>
                                                                <tr>
                                                                    <td>Experties</td>
                                                                    <td id="experties3">@ViewBag.x.experties.ToString()</td>
                                                                    <td>
                                                                        <button id="edit15" data-original-title="Broadcast Message" data-toggle="tooltip" type="button" class="btn btn-sm btn-primary">Edit  <i class="glyphicon glyphicon-edit"></i></button>
                                                                        <a id="ok15" data-original-title="Broadcast Message" data-toggle="tooltip" href="#" type="button" class="btn btn-sm btn-primary">OK <i class="glyphicon glyphicon-ok"></i></a>
                                                                    </td>
                                                                </tr>

                                                                <tr>
                                                                <tr>
                                                                    <td>Qualification</td>
                                                                    <td id="qualification3">@ViewBag.x.qualification.ToString()</td>
                                                                    <td>
                                                                        <button id="edit16" data-original-title="Broadcast Message" data-toggle="tooltip" type="button" class="btn btn-sm btn-primary">Edit  <i class="glyphicon glyphicon-edit"></i></button>
                                                                        <a id="ok16" data-original-title="Broadcast Message" data-toggle="tooltip" href="#" type="button" class="btn btn-sm btn-primary">OK <i class="glyphicon glyphicon-ok"></i></a>
                                                                    </td>
                                                                </tr>

                                                            </tbody>

